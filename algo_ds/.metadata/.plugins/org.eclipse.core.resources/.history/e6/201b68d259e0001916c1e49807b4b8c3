#include <iostream>
using namespace std;
#include<iomanip>

int comparisons=0;

bool linear_search(int arr[],int key)
{
	comparisons=0;

	for(int index=0;index<size;index++)
	{
		comparisons++;
		if(arr[index]==key)
		{
			return true;
		}
		else
		{
			return false;
		}
	}


}





